{
  "expo": {
    "name": "OnlyCats",
    "slug": "onlycats-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["ios", "android", "web"],
    "ios": {
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsLocalNetworking": true
        }
      },
      "bundleIdentifier": "com.onlycats.app"
    },
    "android": {
      "usesCleartextTraffic": true,
      "package": "com.onlycats.app"
    },
    "extra": {
      "apiHost": "192.168.3.5",
      "apiPort": "8080"
    }
  }
}
