{
    "schemes": [
        "http",
        "https"
    ],
    "swagger": "2.0",
    "info": {
        "description": "Match Service for OnlyCats - handles cat search, applications, suggestions, and user preferences",
        "title": "OnlyCats Match Service API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.onlycats.com/support",
            "email": "support@onlycats.com"
        },
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        },
        "version": "2.0"
    },
    "host": "localhost:8084",
    "basePath": "/",
    "paths": {
        "/search/cats": {
            "get": {
                "description": "Search for cats based on various filters",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Search"
                ],
                "summary": "Search for cats",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Species (e.g., cat)",
                        "name": "species",
                        "in": "query"
                    },
                    {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "Breeds",
                        "name": "breeds",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "Minimum age",
                        "name": "age_min",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "Maximum age",
                        "name": "age_max",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "enum": ["male", "female"],
                        "description": "Gender",
                        "name": "gender",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "enum": ["small", "medium", "large"],
                        "description": "Size",
                        "name": "size",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "Maximum search radius in km",
                        "name": "max_radius",
                        "in": "query"
                    },
                    {
                        "type": "boolean",
                        "description": "Good with kids",
                        "name": "good_with_kids",
                        "in": "query"
                    },
                    {
                        "type": "boolean",
                        "description": "Good with pets",
                        "name": "good_with_pets",
                        "in": "query"
                    },
                    {
                        "type": "boolean",
                        "description": "Has special needs",
                        "name": "special_needs",
                        "in": "query"
                    },
                    {
                        "type": "boolean",
                        "description": "Only available cats",
                        "name": "available",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "default": 1,
                        "description": "Page number",
                        "name": "page",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "default": 20,
                        "description": "Items per page",
                        "name": "limit",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "default": "created_at",
                        "enum": ["created_at", "age", "distance"],
                        "description": "Sort by",
                        "name": "sort_by",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "default": "desc",
                        "enum": ["asc", "desc"],
                        "description": "Sort order",
                        "name": "sort_order",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Search results",
                        "schema": {
                            "$ref": "#/definitions/models.SearchResponse"
                        }
                    },
                    "400": {
                        "description": "Invalid request",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
